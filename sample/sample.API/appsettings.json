{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AWS": {
    "S3": {
      "AccessKey": "your-access-key",
      "SecretKey": "your-secret-key",
      "BucketName": "your-production-bucket",
      "Region": "us-east-1",
      "UseLocalStack": false
    },
    "SecretsManager": {
      "AccessKey": "your-access-key",
      "SecretKey": "your-secret-key",
      "Region": "us-east-1",
      "UseLocalStack": false,
      "SecretPrefix": "prod/",
      "EnableCaching": true,
      "CacheExpirationMinutes": 30
    }
  }
}
